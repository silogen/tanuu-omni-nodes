{
  "packages": [
    "go@1.22.1",
    "go-task@3.35.1",
    "path:gcloud#google-cloud-sdk",
    "gh@2.44.1",
    "kubectl@1.29.2",
    "bat@0.24.0",
    "yq-go@4.41.1",
    "kubernetes-helm@3.14.1",
    "teller@1.5.6",
    "omnictl@0.32.2",
    "golint@2020-12-08",
    "yamllint@1.35.1",
    "yamlfmt@0.12.1"
  ],
  "shell": {
    "init_hook": [
      "alias cat='bat --paging never --theme DarkNeon --style plain'",
      "eval \"$(teller sh)\"",
      "gcloud auth list | grep 'silo.ai' || echo 'Error: not logged into a silo.ai glcoud account'"
    ],
    "scripts": {
      "test": [
        "echo \"Error: no test specified\" && exit 0"
      ]
    }
  }
}
